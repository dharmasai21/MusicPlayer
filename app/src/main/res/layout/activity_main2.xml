<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">

    <Button
        android:id="@+id/button"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageButton9"
        android:layout_marginTop="168dp"
        android:onClick="bajagovindam"
        android:text="Bajagovindam"
        android:textSize="12sp" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_marginStart="37dp"
        app:srcCompat="@drawable/music15"
        android:onClick="bajagovindam"/>

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="37dp"
        android:layout_marginTop="30dp"
        android:onClick="vandemataram"
        app:srcCompat="@drawable/music15" />

    <Button
        android:id="@+id/button7"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/imageButton10"
        android:layout_alignTop="@+id/button"
        android:text="vandemataram"
        android:onClick="vandemataram"
        android:textSize="12sp" />

</RelativeLayout>